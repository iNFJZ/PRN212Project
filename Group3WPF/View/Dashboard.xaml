<Window x:Class="Group3WPF.View.Dashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Group3WPF.View"
        mc:Ignorable="d"
        Title="Dashboard" Height="450" Width="800"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Image Grid.Row="0" Source="D:\projectprnteam3\PRN212Project\BackGroud\Blue White Modern Ocean Background Instagram Story.png" Stretch="Fill" />
        <Button x:Name="ProductButton" FontWeight="Bold" BorderThickness="0" Background="#007ACC" Foreground="White"  Content="Danh Sách Sản phẩm" HorizontalAlignment="Left" Margin="337,264,0,0" VerticalAlignment="Top" Width="145" Click="Product_Click" RenderTransformOrigin="0.521,-0.134" Height="26"/>
        <Button x:Name="CategoryButton" FontWeight="Bold" BorderThickness="0" Background="#007ACC" Foreground="White"  Content="Danh Sách Danh Mục" HorizontalAlignment="Left" Margin="337,311,0,0" VerticalAlignment="Top" Width="145" Click="Category_Click" RenderTransformOrigin="0.521,-0.134" Height="26"/>
        <Button x:Name="PurchaseButton" FontWeight="Bold" BorderThickness="0" Background="#007ACC" Foreground="White"  Content="Danh Sách Đơn Hàng" HorizontalAlignment="Left" Margin="337,217,0,0" VerticalAlignment="Top" Width="145" Click="Purchase_Click" RenderTransformOrigin="0.521,-0.134" Height="26"/>
        <Button x:Name="PurchaseOrderLineButton" FontWeight="Bold" BorderThickness="0" Background="#007ACC" Foreground="White"  Content="Chi Tiết Đơn Hàng" HorizontalAlignment="Left" Margin="337,177,0,0" VerticalAlignment="Top" Width="145" Click="Purchase_Detail_Click" RenderTransformOrigin="0.521,-0.134" Height="26"/>
        <Button x:Name="SuppliesButton" FontWeight="Bold" BorderThickness="0" Background="#007ACC" Foreground="White"  Content="Danh Sách Nhà Cung Cấp" HorizontalAlignment="Left" Margin="337,134,0,0" VerticalAlignment="Top" Width="145" Click="Supplies_Click" RenderTransformOrigin="0.521,-0.134" Height="26"/>
    </Grid>
</Window>
